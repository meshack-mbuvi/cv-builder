{
  "name": "cv-builder",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "run:test": "NODE_ENV=test nyc mocha --require @babel/register test/**/*.js",
    "start:api:dev": "NODE_ENV=development DEBUG=log nodemon --exec babel-node src/index.js",
    "start:dev": "nodemon --exec babel-node src/index",
    "sequelize:migrate": "NODE_ENV=test sequelize db:migrate",
    "installDep": "npm install",
    "clean": "rimraf ./dist ",
    "build-src": "babel ./src -d dist --copy-files",
    "build": "npm run clean && npm run build-src",
    "start": "npm run build && node dist/index.js",
    "sequelize:undo:seed": "NODE_ENV=test sequelize db:seed:undo:all",
    "test": "npm run sequelize:migrate && npm run run:test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "@babel/runtime": "^7.4.5",
    "@hapi/joi": "^15.0.3",
    "babel-loader": "^8.0.0-beta.6",
    "babel-node": "0.0.1-security",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cors": "^2.8.5",
    "cov": "^0.1.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^9.0.0",
    "loggery": "^1.0.2",
    "nodemon": "^2.0.1",
    "password-hash": "^1.2.2",
    "pg": "^7.15.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.3",
    "sequelize-cli": "^5.5.1",
    "swagger-ui-express": "^4.1.2",
    "yaml": "^1.7.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.9",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "supertest": "^4.0.2"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/lib/*.js",
      "src/sequelize/*"
    ],
    "extension": [
      ".js"
    ]
  }
}
